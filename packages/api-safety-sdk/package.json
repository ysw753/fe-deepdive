{
  "name": "api-safety-sdk",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./adapters/rhf": {
      "import": "./dist/adapters/rhf.js",
      "require": "./dist/adapters/rhf.cjs",
      "types": "./dist/adapters/rhf.d.ts"
    },
    "./error-helpers": {
      "import": "./dist/error-helpers.js",
      "require": "./dist/error-helpers.cjs",
      "types": "./dist/error-helpers.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest run"
  },
  "peerDependencies": {
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "zod": "^4.1.5"
  }
}
